<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yang.myalarm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlarmListActivity" />
        <activity android:name=".AlarmDetail" />
        <activity android:name=".AlarmDetailActivity" />
        <activity android:name=".AlarmSoundActivity" />
        <activity android:name=".SoundMenuActivity" />
        <activity android:name=".SoundFromRecodeActivity" />
        <activity android:name=".SoundFromPCActivity" />
        <activity android:name=".SoundFromServerActivity" />
        <activity android:name=".SoundFromYoutubeActivity" />
        <activity android:name=".AlarmPlayActivity"></activity>
    </application>

</manifest>